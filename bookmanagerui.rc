<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui name="bookmanager" version="6">
<MenuBar>
  <Menu name="file">
    <text>&amp;File</text>
    <Action name="open" />
    <Separator />
    <Merge />
    <Separator />
    <Action name="file_quit" />
    <Separator/>
  </Menu>
  <Menu name="Collection">
    <text>&amp;Collection</text>
    <Action name="showCollection" />
    <Separator />
    <Action name="ghns" />
    <Action name="import" />
    <Action name="remove" />
    <Merge />
  </Menu>
  
  <Merge />
</MenuBar>
<ToolBar name="toolbar">
  <text>Main Toolbar </text>
  <Action name="open" />	
  <Action name="ghns" />
  <Merge />
</ToolBar>
</kpartgui>